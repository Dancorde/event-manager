{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend of event manager",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --verbose --forceExit",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:coverage": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit --coverage"
  },
  "author": "Danilo Henrique Cordeio",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "custom-env": "^2.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "coverage"
    ]
  }
}
